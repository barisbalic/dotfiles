# Shorthand git clone: gcl barisbaris/dotfiles
# Unnecessary if you have hub installed
gcl() { 
  git clone git@github.com:$1
}

# Grep
g() {
  grep $1
}

# Grep without
ng() {
  grep -v $1
}

# Add your key to a remote servers authorised keys
authme() {
  ssh $1 'cat >>~/.ssh/authorized_keys' <~/.ssh/id_rsa.pub
}
